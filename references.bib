	keywords = {3d printing, Batteries, Materials selection, Sensors, Super capacitors, Triboelectric nanogenerators},
	pages = {477--502},
	file = {Divakaran et al. - 2022 - Comprehensive review on various additive manufactu.pdf:/Users/alex/Zotero/storage/4MCLMAKL/Divakaran et al. - 2022 - Comprehensive review on various additive manufactu.pdf:application/pdf;ScienceDirect Snapshot:/Users/alex/Zotero/storage/J2AQ6EIS/S0278612522000024.html:text/html},

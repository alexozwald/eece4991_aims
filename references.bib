	note = {ISSN: 2380-6923},
	keywords = {Antenna measurements, Three-dimensional displays, 24 GHz, 3D antennas, high frequency antennas, ISM band, Patch antennas, Solid modeling, Surface impedance, Transmission line antennas},
	pages = {470--474},
	file = {IEEE Xplore Abstract Record:/Users/alex/Zotero/storage/YB7I8CLK/7031779.html:text/html;IEEE Xplore Full Text PDF:/Users/alex/Zotero/storage/FD7X25UD/Sravani and Rao - 2015 - Design of 3D Antennas for 24 GHz ISM Band Applicat.pdf:application/pdf},
}

